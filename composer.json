{
    "name": "samjoyce777/marketing",
    "description": "Marketing package",
    "version": "1.0.4",
    "type": "library",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:Global4Communications/smsmanager.git"
        }
    ],
    "require": {
        "Global4Communications/sms-manager": "1.*",
        "mandrill/mandrill": "1.*",
        "nesbot/carbon": "1.*",
        "elmhurstprojects/core-for-projects": ">+=.0.11"
    },
    "license": "MIT",

    "autoload": {
        "psr-4": {
            "SamJoyce777\\Marketing\\": "src/",
            "ElmhurstProjects\\CommunicationsExampleSimple\\": "example-simple/",
            "ElmhurstProjects\\CommunicationsExampleReal\\": "example-real/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SamJoyce777\\Marketing\\Providers\\MarketingServiceProvider",
                "ElmhurstProjects\\CommunicationsExampleSimple\\Providers\\ExampleSimpleServiceProvider",
                "ElmhurstProjects\\CommunicationsExampleReal\\Providers\\ExampleRealServiceProvider"
            ]
        }
    }
}
